<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              style="@style/SNllStyle">
    <ImageView
            android:id="@+id/ivUser_FN"
            style="@style/AVC_Detailes_Subitem_Icon_IV"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/ic_phone_grey600_24dp" android:layout_gravity="center_vertical"/>
    <TextView
            android:id="@+id/tvNumber_FN"
        style="@style/SNtvCountryStyle"
            android:text="@string/unknown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>